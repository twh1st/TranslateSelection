{
	"default_locale": "en",
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' https://api.microsofttranslator.com/V2/Ajax.svc ; object-src 'self' ",
	"version": "1.1.8.4",
	"name": "__MSG_name__",
	"description": "__MSG_description__",
	"icons": {
		"16": "icons/ico16.png",
		"19": "icons/ico19.png",
		"32": "icons/ico32.png",
		"48": "icons/ico48.png",
		"128": "icons/ico128.png"
	},
	"options_page": "options.html",
	//"background_page": "background.html",
	"background": {
	  "page": "background.html"
	},
	"browser_action": {
		"default_icon": "icons/ico19.png",
		"default_popup" : "popup.html"
	},
	"permissions": ["tabs","http://*/", "https://*/","http://*/*","contextMenus"]
	//"permissions": ["tabs","<all_urls>","http://*/*","contextMenus"]
}
